
public class Recursividade {

	public static void main(String[] args) {
		String[] alunos = new String[] {"Nathany", "José", "Gabriel"};
		
		iterarNomes(alunos);
		
	}
	
	static void iterarNomes(String[] alunos) { // nao precisa chamar alunos necessariamente
		for(int i = 0; i < alunos.length; i++) {
			System.out.println("Aluno: " + alunos[i]);
			
		}
		
		
	}

	
}
