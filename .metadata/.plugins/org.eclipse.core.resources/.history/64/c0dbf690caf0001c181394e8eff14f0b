
public class VetoresMaisDe2Dimensoes {
	public static void main(String[] args) {
		
		Double[] faturamentoJaneiro2022 = new Double[] {15000.0, 20000.0, 30000.0, 40000.0};
		Double[] faturamentoFevereiro2022 = new Double[] {20000.0, 50000.0, 70000.0, 90000.0};
		Double[] faturamentoMarço2022 = new Double[] {30000.0, 90000.0, 70000.0, 120000.0};
		
		Double[] faturamentoJaneiro2023 = new Double[] {15000.0, 20000.0, 30000.0, 40000.0};
		Double[] faturamentoFevereiro2023 = new Double[] {20000.0, 50000.0, 70000.0, 90000.0};
		Double[] faturamentoMarço2023 = new Double[] {30000.0, 90000.0, 70000.0, 120000.0};
		
		Double[][] faturamentoAnual2022 = new Double [][] {faturamentoJaneiro2022, faturamentoFevereiro2022, faturamentoMarço2022};
		
		Double[][] faturamentoAnual2023 = new Double [][] {faturamentoJaneiro2023, faturamentoFevereiro2023, faturamentoMarço2023};

		Double[][][] faturamentoBienal = new Double[][][] {faturamentoAnual2022, faturamentoAnual2023 };
		
		
		for(int i = 0; i < faturamentoAnual.length ; i++) {
			System.out.println("Mês: " + (i+1));
			
			Double[] mes = faturamentoAnual[i];
			
			for(int y = 0; y < mes.length ; y++) {
				Double dia = mes[y]; //double dia = faturamentoAnual[i][y];
				System.out.println("Dia " + (y+1) + ": " + dia);
			}
		}
		
		
	}
}

		
	
	}
}
